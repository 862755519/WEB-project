.container{
  padding-left: 15rpx;
  padding-right: 15rpx;
}
.end{
  height: 50rpx;
  text-align: center;
  margin-top: 40rpx;
  color: #999;
}
.logo{
  width: 130rpx;
  height: 130rpx;
  border-radius: 65px;
  margin-top: 10rpx;
 
}
.text_con{
  height: 150rpx;
 
}
.name{
  overflow: hidden;
  height: 50rpx;
  padding-left: 10rpx;
  color: #121212;
  font-size: 35rpx;
  margin-top: 10rpx;
}
.tab_con{
  height: 55rpx;
  margin-top: 20rpx;
}
.tab_con span{
  border: 1px solid #D9D9D9;
  color: #616161;
  font-size: 27rpx;
  margin-left: 10rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  border-radius: 8rpx;
}
.but{
  width: 150rpx;
  height: 60rpx;
  line-height: 55rpx;
  border-radius: 3px;
  margin-top: 40rpx;
  font-size: 27rpx;
  padding: 0px;
  color: #fff;
  background: #2492F4;
  border: 1px solid #2492F4;
}
.row {
  margin-top: 30rpx;
  border: 1px solid #D9D9D9;
  border-radius: 5rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}