.container{
  padding-left: 15rpx;
  padding-right: 15rpx;
}
.accomplish{
  color: #999;
  font-size: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-top: 20rpx;
}
.logo{
  width: 140rpx;
  height: 140rpx;
  border-radius: 5px;
}
.text_con{
  height: 150rpx;
 
}
.load{
  width: 100%;
  height: 60rpx;
  text-align: center;
  
}
.name{
  overflow: hidden;
  height: 50rpx;
  padding-left: 10rpx;
  color: #121212;
  font-size: 35rpx;
  margin-top: 10rpx;
}
.tab_con{
  height: 55rpx;
  margin-top: 20rpx;
}
.tab_con span{
  border: 1px solid #D9D9D9;
  color: #616161;
  font-size: 30rpx;
  margin-left: 10rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  border-radius: 5rpx;
}
.but{
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 3px;
  margin-top: 40rpx;
  font-size: 30rpx;
  color: #fff;
  background: #2492F4;
  border: 1px solid #2492F4;
}
.row {
  margin-top: 30rpx;
  border: 1px solid #D9D9D9;
  border-radius: 5rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}